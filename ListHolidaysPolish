Sub ListHolidays()Dim i As LongDim wielkanoc As DateDim k As Longk = 1For i = 2003 To 2020    wielkanoc = WorksheetFunction.Floor(DateSerial(i, 5, Day(Minute(i / 38) / 2 + 56) - CInt(Minute(i / 38) < 10)), 7) - 34    Cells(k, 1) = DateSerial(i, 1, 1)  'Nowy rok    k = k + 1    Cells(k, 1) = DateSerial(i, 1, 6)     'Trzech Króli (Objawienie Panskie)    k = k + 1    Cells(k, 1) = wielkanoc    k = k + 1    Cells(k, 1) = wielkanoc + 1     'Poniedzialek Wielkanocny    k = k + 1    Cells(k, 1) = DateSerial(i, 5, 1)     'Swieto pracy    k = k + 1    Cells(k, 1) = DateSerial(i, 5, 3)     'Konstytucja 3 Maja    k = k + 1    Cells(k, 1) = wielkanoc + 49     'Zielone Swiatki (Zeslanie Ducha Swietego)    k = k + 1    Cells(k, 1) = wielkanoc + 60     'Boze Cialo    k = k + 1    Cells(k, 1) = DateSerial(i, 8, 15)     'Wniebowziecie NMP    k = k + 1    Cells(k, 1) = DateSerial(i, 11, 1)     'Wszystkich Swietych    k = k + 1    Cells(k, 1) = DateSerial(i, 11, 11)     'Swieto Niepodleglosci    k = k + 1    Cells(k, 1) = DateSerial(i, 12, 25)     'Boze Narodzenie    k = k + 1    Cells(k, 1) = DateSerial(i, 12, 26)     'Szczepana    k = k + 1Next iEnd Sub
